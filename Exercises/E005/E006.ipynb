    "def e006(text):\n",
    "    a=\"\"\n",
    "\n",
    "    c=len(text)\n",
    "    c*=-1\n",
    "    counter=0\n",
    "    while(counter>c):\n",
    "        counter-=1\n",
    "        a+=text[counter]\n",
    "    print(a==text)\n",
    "\n",
    "e006(input(\"enter test: \").lower())\n",
    "#تابع .lower از افزونه grepper به دست آمد"
