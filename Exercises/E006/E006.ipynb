    "def E006(txt):\n",
    "    up=''\n",
    "    low=''\n",
    "    sign=''\n",
    "    for chr in txt:\n",
    "        if(chr.islower()):\n",
    "            low+=chr\n",
    "        elif(chr.isupper()):\n",
    "            up+=chr\n",
    "        else:\n",
    "            sign+=chr\n",
    "    print(up)\n",
    "    print(low)\n",
    "    print(sign)\n",
    "\n",
    "E006(\"*+_Ab@Owm-\")"
   ]
